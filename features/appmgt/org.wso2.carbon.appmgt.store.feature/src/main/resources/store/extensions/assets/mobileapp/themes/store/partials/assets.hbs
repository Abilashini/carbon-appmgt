<div class="raw">


{{#if assets}}


    {{#each assets}}

    <div class="col-md-2 app-asset-wrapper">
        <div class="app-asset">
            <a href="{{url "/assets"}}/{{type}}/{{id}}">
            <img class="app-asset-icon"
                 src="{{#if attributes.images_thumbnail}}{{attributes.images_thumbnail}}{{/if}}">
            </a>
            <div class="app-asset-info">
                <div class="app-asset-name">
                    {{attributes.overview_displayName}}
                </div>
                <div class="app-asset-provider">
                    {{attributes.overview_provider}}
                </div>
            </div>

            <div class="app-asset-icon-info">
                <img class="app-asset-icon-info-platform" src="http://icons.iconarchive.com/icons/igh0zt/ios7-style-metro-ui/512/MetroUI-Folder-OS-OS-Android-icon.png">
                <img class="app-asset-icon-info-platform" src="http://icons.iconarchive.com/icons/igh0zt/ios7-style-metro-ui/512/MetroUI-Folder-OS-OS-Android-icon.png">
            </div>

            <span class="app-asset-rating"> </span>
        </div>
    </div>

    {{/each}}

{{/if}}


</div>
