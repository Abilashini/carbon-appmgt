{{#if .}}
<div class="row">
    <h3>Recent Apps</h3>
    <hr>


    {{#slice . count="20" size="20"}}
        {{#each .}}
            {{#compare  attributes.overview_advertiseOnly "true" operator="=="}}
            <a href="{{url "/t"}}/{{attributes.overview_appTenant}}/assets/{{type}}/{{attributes.overview_advertisedAppUuid}} ">
            {{else}}
            <a href="{{tenantedUrl "/assets"}}/{{type}}/{{id}}">
            {{/compare}}
            <div class="row">

                    <div class="col-md-8">
                        <div class="recent-app-assets-name">{{attributes.overview_displayName}}</div>
                        <div class="recent-app-asset-provider">{{attributes.overview_provider}}</div>
                        <span class="app-assets-rating"> {{#assetRating rating}}{{/assetRating}} </span>
                    </div>
                    <div class="col-md-4">
                        <img class="col-md-12"
                             src="{{#if attributes.images_thumbnail}}{{attributes.images_thumbnail}}{{/if}}">
                    </div>
            </div>
            </a>
            <hr>
        {{/each}}
    {{/slice}}


</div>
{{/if}}